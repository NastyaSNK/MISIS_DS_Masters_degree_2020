      "source": [
        "#библиотечный тест на простоту\n",
        "res_sum=0\n",
        "kol_include=0\n",
        "find_numbers=[]\n",
        "start_time=time.time() #стартовое время работы алгоритма\n",
        "all_str_with_nums= get_number_consider_s(s,d)\n",
