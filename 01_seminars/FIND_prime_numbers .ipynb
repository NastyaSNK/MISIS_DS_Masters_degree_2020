        "def miller_rabin(n, r):\n",
        "    import random\n",
        "    # Если число четное кроме 2, оно составное\n",
        "    if n == 2:\n",
        "        return True\n",
        "    if n % 2 == 0:\n",
        "        return False\n",
        "\n",
        "    # Последовательное  деление\n",
        "    s, t = 0, n - 1\n",
        "    while t % 2 == 0:\n",
        "        #степень\n",
        "        s += 1\n",
        "        #число\n",
        "        t //= 2\n",
        "    #Кол-во повторений алгоритма\n",
        "    for _ in range(r):\n",
        "        #Выбор случайного числа\n",
        "        a = random.randrange(2, n - 2)\n",
        "        #Проверка условия 1\n",
        "        x = pow(a, t, n)\n",
        "        if x == 1 or x == n - 1:\n",
        "            continue\n",
        "        #Проверка условия 2\n",
        "        for k in range(s - 1):\n",
        "            x = pow(x, 2, n)\n",
        "            if x == 1:\n",
        "                return False\n",
        "            if x == n - 1:\n",
        "                break\n",
        "        else:\n",
        "            return False\n",
        "    return True\n",
